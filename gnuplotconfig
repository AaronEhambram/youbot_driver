set terminal postscript eps enhanced color solid 
set terminal postscript landscape
set output "|ps2pdf - jointDataPlot.pdf"
#set output "|epstopdf --filter > jointDataPlot.pdf"

set title "joint angle"
set ylabel "angle [rad]"
set xlabel "time [milliseconds]"
set grid
set offsets 0, 0, 1, 1
plot  "jointDataTrace" using 1:2 title 'angle setpoint' with lines, "jointDataTrace" using 1:9 title 'sensed angle' with lines

set title "joint encoder ticks"
set ylabel "encoder ticks"
set xlabel "time [milliseconds]"
set grid
set offsets 0, 0, 0.01, 0.01
plot  "jointDataTrace" using 1:8 title 'encoder setpoint' with lines, "jointDataTrace" using 1:10 title 'sensed encoder' with lines

set title "joint velocity"
set ylabel "velocity [rad/s]"
set xlabel "time [milliseconds]"
set grid
set offsets 0, 0, 0.2, 0.2
plot  "jointDataTrace" using 1:3 title 'velocity setpoint' with lines, "jointDataTrace" using 1:11 title 'sensed velocity' with lines

set title "joint rounds per minute"
set ylabel "rpm"
set xlabel "time [milliseconds]"
set grid
set offsets 0, 0, 1, 1
plot  "jointDataTrace" using 1:4 title 'rpm setpoint' with lines, "jointDataTrace" using 1:12 title 'sensed rpm' with lines

set title "joint current"
set ylabel "current [ampere]"
set xlabel "time [milliseconds]"
set grid
set offsets 0, 0, 0.1, 0.1
plot  "jointDataTrace" using 1:5 title 'current setpoint' with lines, "jointDataTrace" using 1:13 title 'sensed current' with lines

set title "joint torque"
set ylabel "torque [Nm]"
set xlabel "time [milliseconds]"
set grid
set offsets 0, 0, 0.01, 0.01
plot  "jointDataTrace" using 1:6 title 'torque setpoint' with lines, "jointDataTrace" using 1:14 title 'sensed torque' with lines

set title "joint PWM"
set ylabel "PWM"
set xlabel "time [milliseconds]"
set grid
set offsets 0, 0, 0.01, 0.01
plot  "jointDataTrace" using 1:7 title 'PWM setpoint' with lines


set multiplot 
set xlabel ""
set title ""

set size 1, 0.25
set origin 0,0
set yrange [-0.1:1.1]
set ytics 1
set ylabel "over current"
set grid
plot "jointDataTrace" using 1:15 title '' with lines

set size 1, 0.25
set origin 0.0,0.25
set yrange [-0.1:1.1]
set ytics 1
set ylabel "under voltage"
set grid
plot "jointDataTrace" using 1:16 title '' with lines

set size 1, 0.25
set origin 0.0,0.5
set yrange [-0.1:1.1]
set ytics 1
set ylabel "over voltage"
set grid
plot "jointDataTrace" using 1:17 title '' with lines

set size 1, 0.25
set origin 0.0,0.75
set yrange [-0.1:1.1]
set ytics 1
set ylabel "over temperature"
set grid
plot "jointDataTrace" using 1:18 title '' with lines

unset multiplot


